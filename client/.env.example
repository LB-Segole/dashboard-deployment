# VoiceAI Environment Variables Example
# Copy this to .env.local and fill in the values

# API Configuration
NEXT_PUBLIC_API_URL=https://api.voiceai.tech/v1
NEXT_PUBLIC_WS_URL=wss://api.voiceai.tech/v1/ws

# Authentication
NEXT_PUBLIC_AUTH0_DOMAIN=your-domain.auth0.com
NEXT_PUBLIC_AUTH0_CLIENT_ID=your-client-id
NEXT_PUBLIC_AUTH0_AUDIENCE=your-audience

# Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
NEXT_PUBLIC_SENTRY_DSN=https://xxxxxxx@xxxxxx.ingest.sentry.io/xxxxxx

# Feature Flags
NEXT_PUBLIC_ENABLE_EXPERIMENTAL_FEATURES=false
NEXT_PUBLIC_ENABLE_CALL_RECORDING=true

# Third-party Services
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxx
NEXT_PUBLIC_DEEPGRAM_API_KEY=xxxxxxxxxxxxxxxx
NEXT_PUBLIC_SIGNALWIRE_SPACE_URL=your-space.signalwire.com